# Railway Environment Variables Example
# Copy this file and set these variables in your Railway dashboard

# OpenAI API Key (REQUIRED)
OPENAI_API_KEY=your_openai_api_key_here

# Database URLs (Automatically set by Railway PostgreSQL service)
DATABASE_URL=postgresql://user:password@host:port/database
ASYNC_DATABASE_URL=postgresql+asyncpg://user:password@host:port/database

# Redis URL (Automatically set by Railway Redis service)
REDIS_URL=redis://default:password@host:port

# JWT Secret Key (Generate a secure random string)
JWT_SECRET_KEY=your_jwt_secret_key_here

# Application Settings
ENVIRONMENT=production
DEBUG=false
LOG_LEVEL=INFO
HOST=0.0.0.0
PORT=8001
MAX_WORKERS=4
TIMEOUT_SECONDS=30
API_RATE_LIMIT=100
FORCE_HTTPS=true 